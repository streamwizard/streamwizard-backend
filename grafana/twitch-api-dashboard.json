          "query": "total = from(bucket: \"${bucket}\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"twitch_api_request\")\n  |> filter(fn: (r) => r[\"_field\"] == \"count\")\n  |> filter(fn: (r) => r[\"app\"] =~ /${app}/)\n  |> group()\n  |> sum()\n  |> duplicate(column: \"_stop\", as: \"_time\")\n  |> set(key: \"metric\", value: \"total\")\n\nsuccess = from(bucket: \"${bucket}\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"twitch_api_request\")\n  |> filter(fn: (r) => r[\"_field\"] == \"count\")\n  |> filter(fn: (r) => r[\"status\"] =~ /^2[0-9][0-9]$/)\n  |> filter(fn: (r) => r[\"app\"] =~ /${app}/)\n  |> group()\n  |> sum()\n  |> duplicate(column: \"_stop\", as: \"_time\")\n  |> set(key: \"metric\", value: \"success\")\n\njoin(\n  tables: {total: total, success: success},\n  on: [\"_time\"]\n)\n  |> map(fn: (r) => ({\n      _time: r._time,\n      _value: if r._value_total > 0 then (float(v: r._value_success) / float(v: r._value_total)) * 100.0 else 0.0\n    }))\n  |> limit(n: 1)",
